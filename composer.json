{
    "name": "arillo/silverstripe-multidb",
    "description":
        "Simple DataObject proxy helping to persist records in multiple databases",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/arillo/silverstripe-multidb",
    "keywords": ["silverstripe", "multidb"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Fischer",
            "email": "sf@arillo.net"
        }
    ],
    "require": {
        "silverstripe/framework": "^4",
        "catfan/medoo": "^1.5"
    },
    "extra": {
        "installer-name": "multidb"
    },
    "autoload": {
        "psr-4": {
            "Arillo\\MultiDB\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/arillo/silverstripe-multidb/issues"
    }
}
